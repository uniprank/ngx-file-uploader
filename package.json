{
  "name": "ngx-file-uploader",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --project=TestCases --host=0.0.0.0",
    "build": "ng build NgxFileUploader --prod",
    "build:testcases": "ng build --project=TestCases --prod",
    "build:development": "ng build --watch",
    "test": "ng test NgxFileUploader --watch=false --code-coverage",
    "test:travis": "ng test NgxFileUploader --watch=false --code-coverage --no-progress --browsers=ChromeHeadlessNoSandbox",
    "test:watch": "ng test NgxFileUploader --watch --code-coverage",
    "lint": "ng lint NgxFileUploader",
    "publish": "cd dist/ngx-file-uploader && npm publish --access public",
    "postinstall": "ngcc",
    "test:changelog": "changelog -M",
    "report-coverage": "codecov -t $CODECOV_TOKEN",
    "release:major": "changelog -M && git add CHANGELOG.md && git commit -m 'updated CHANGELOG.md' && npm version major && git push origin && git push origin --tags",
    "release:minor": "changelog -m && git add CHANGELOG.md && git commit -m 'updated CHANGELOG.md' && npm version minor && git push origin && git push origin --tags",
    "release:patch": "changelog -p && git add CHANGELOG.md && git commit -m 'updated CHANGELOG.md' && npm version patch && git push origin && git push origin --tags"
  },
  "private": false,
  "dependencies": {
    "@angular/animations": "~10.0.14",
    "@angular/common": "~10.0.14",
    "@angular/compiler": "~10.0.14",
    "@angular/core": "~10.0.14",
    "@angular/forms": "~10.0.14",
    "@angular/platform-browser": "~10.0.14",
    "@angular/platform-browser-dynamic": "~10.0.14",
    "@angular/router": "~10.0.14",
    "github-markdown-css": "^4.0.0",
    "ngx-markdown": "^9.0.0",
    "prism-themes": "^1.3.0",
    "prismjs": "^1.19.0",
    "rxjs": "~6.5.4",
    "stickyfilljs": "^2.1.0",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1000.8",
    "@angular-devkit/build-ng-packagr": "~0.1000.8",
    "@angular/cli": "~10.0.8",
    "@angular/compiler-cli": "~10.0.14",
    "@angular/language-service": "~10.0.14",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "codelyzer": "^6.0.0",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.3.16",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~3.3.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "karma-junit-reporter": "^2.0.1",
    "ng-packagr": "^10.0.0",
    "protractor": "~7.0.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~3.9.7"
  }
}
